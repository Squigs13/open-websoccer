{% if breadcrumbItems is defined %}
<ol class="breadcrumb">
	{% for breadcrumbItemKey,breadcrumbItemLabel in breadcrumbItems %}
		{% if loop.last %}
		<li class="active">{{ breadcrumbItemLabel }}</li>
		{% else %}
		<li>
			<a href="{{ env.getInternalUrl(breadcrumbItemKey) }}">{{ breadcrumbItemLabel }}</a> 
		</li>
		{% endif %}
	{% endfor %}
</ol>
{% endif %}